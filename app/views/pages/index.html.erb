<% content_for :top_content do %>
  <div class="jumbotron">
    <div class="container">
      <h1>Welcome to IMDB</h1>
      <p>Register to get started.</p>
      <p>
        <%= link_to 'Register', new_user_registration_path, class: 'btn btn-primary btn-lg' %>
      </p>
    </div>
  </div>

<% if current_user.try(:admin?) %>
	<p>I'm admin</p>
<% else %>
	<p>I'm not an Admin</p>
<% end %>
<% end %>